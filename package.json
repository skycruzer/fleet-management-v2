{
  "name": "fleet-management-v2",
  "version": "0.1.0",
  "private": true,
  "description": "Modern fleet management system built with Next.js 15, TypeScript, and Supabase",
  "author": "Maurice (Skycruzer)",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "validate": "npm run type-check && npm run lint && npm run format:check",
    "validate:naming": "node scripts/validate-naming.mjs",
    "prepare": "husky install",
    "db:types": "supabase gen types typescript --project-id wgdmgvonqysflwdiiols > types/supabase.ts",
    "db:migration": "supabase migration new",
    "db:deploy": "supabase db push"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.4",
